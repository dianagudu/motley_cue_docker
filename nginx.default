server {
    listen 80;
    listen [::]:80;

    server_name ab2ccf04-eb74-4090-93e7-e2abc619ca00.ka.bw-cloud-instance.org;
    server_tokens off;

    location /.well-known/acme-challenge/ {
        root /var/www/certbot;
    }

    location / {
        return 301 https://ab2ccf04-eb74-4090-93e7-e2abc619ca00.ka.bw-cloud-instance.org$request_uri;
    }
}
